{
  "name": "stull-atlas",
  "version": "3.5.0",
  "description": "A Computational Ceramic Glaze Explorer â€” 114 years after Stull",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postbuild": "node scripts/postbuild.mjs",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:skip-build": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -SkipBuild",
    "deploy:dev": "powershell -ExecutionPolicy Bypass -File scripts/deploy-dev.ps1",
    "deploy:dev:skip-build": "powershell -ExecutionPolicy Bypass -File scripts/deploy-dev.ps1 -SkipBuild",
    "health": "powershell -ExecutionPolicy Bypass -File scripts/health-check.ps1",
    "failover": "powershell -ExecutionPolicy Bypass -File scripts/failover-deploy.ps1",
    "failover:restore": "powershell -ExecutionPolicy Bypass -File scripts/failover-deploy.ps1 -Restore",
    "setup": "powershell -ExecutionPolicy Bypass -File scripts/setup-supabase.ps1",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "@tauri-apps/api": "^2.10.1",
    "decimal.js": "^10.4.3",
    "density-clustering": "^1.3.0",
    "immer": "^10.0.3",
    "plotly.js-gl2d-dist-min": "^2.27.0",
    "plotly.js-gl3d-dist-min": "^2.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^7.13.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-plotly.js": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^28.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.1.0"
  }
}
